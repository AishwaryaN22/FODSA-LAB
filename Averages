import pandas as pd
import numpy as np
data = {
    "Name": ["Alice", "Bob", "Charlie", "David"],
    "Marks": [75, 85, 90, 80]
}
df = pd.DataFrame(data)
print("Student Data")
print(df)
average_pandas = df["Marks"].mean()
print("\nAverage using Pandas:", average_pandas)
average_numpy = np.mean(df["Marks"])
print("Average using NumPy:", average_numpy)
if average_pandas == average_numpy:
    print("\nBoth methods give the same result!")
else:
    print("\nResults are different.")
