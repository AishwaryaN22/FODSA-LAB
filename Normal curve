import numpy as np
import matplotlib.pyplot as plt
from scipy.stats import norm
mean = 0 
std_dev = 1 
x = np.linspace(-5, 5, 100)
y = norm.pdf(x, mean, std_dev)
plt.plot(x, y, color='blue')
plt.title("Normal Distribution Curve")
plt.xlabel("Values")
plt.ylabel("Probability Density")
plt.grid()
plt.show()
