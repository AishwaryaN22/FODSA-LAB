# Sample data
numbers = [1, 2, 2, 3, 3, 3, 4, 4, 4, 4]

# Create empty dictionary
frequency = {}

# Count frequency
for num in numbers:
    if num in frequency:
        frequency[num] += 1
    else:
        frequency[num] = 1

# Print frequency distribution
print("Frequency Distribution:")
for key, value in frequency.items():
    print(key, ":", value)
